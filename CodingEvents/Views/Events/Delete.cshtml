<h1>Delete Events</h1>

<form method="post">
    @foreach (var evt in ViewBag.events)
    {
    <div class="form-group">
        <label>
            <span>@evt.Name</span>
            <input type="checkbox" name="eventIds" value="@evt.Id"class="form-control" >
        </label>
    </div>
    }
    <input type="submit"name="Delete selected events"class="btn btn-danger">
</form>
